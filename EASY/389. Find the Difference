class Solution:
    def findTheDifference(self, s: str, t: str) -> str:
        
        s_dict = Counter(s)
        t_dict = Counter(t)

        if len(s) > len(t):
            for i in s_dict.keys():
                if s_dict[i] != t_dict[i]:
                    return i
        else:
            for i in t_dict.keys():
                if s_dict[i] != t_dict[i]:
                    return i
