class Solution:
    def findErrorNums(self, nums: List[int]) -> List[int]:
        
        ans_set = set([i+1 for i in range(len(nums))])
        nums_dict = {}
        for index, value in enumerate(nums):
            if value not in nums_dict:
                nums_dict[value] = 1
            else:
                nums_dict[value] += 1
        
        # Find duplicated number
        duplicated = [num for num, count in nums_dict.items() if count == 2][0]
        
        # Find missing number: element in ans_set but not in nums set
        missing = list(ans_set - set(nums))[0]
        
        return [duplicated, missing]
    
