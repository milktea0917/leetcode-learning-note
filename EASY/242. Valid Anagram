# Better Version
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        s = list(s)
        counter_s = Counter(s)
        t = list(t)
        counter_t = Counter(t)
        for key in counter_s.keys():
            if counter_s[key]!=counter_t[key]:
                return False
        return True

# Brute Force 
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        if len(s) != len(t):
            return False
        s = list(s)
        t = list(t)
        for i in s:
            if i in t:
                t.remove(i)
            else:
                return False
        return True
