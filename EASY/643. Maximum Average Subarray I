class Solution(object):
    def findMaxAverage(self, nums, k):
        temp_sum = sum(nums[:k])
        final_sum = temp_sum
        for i in range(k, len(nums)):
            temp_sum += nums[i] - nums[i - k]
            if temp_sum > final_sum:
                final_sum = temp_sum
        return final_sum / float(k)
