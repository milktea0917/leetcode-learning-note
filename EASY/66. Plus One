class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        
        # list to integer
        temp = [str(x) for x in digits]
        digits_int = int("".join(temp))
        
        # +=1
        digits_int += 1
        
        # int to list
        temp = list(str(digits_int))
        digits_final = [int(x) for x in temp]

        return digits_final
